{
    "name": "mirzahilmi/saphpi",
    "description": "Saphpi, pronounced `sapi` is a web application framework that implements the MVC pattern. It's inspired by many popular frameworks and is designed to be explicit with batteries included.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mirza Hilmi",
            "email": "exquisitemirza@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "mirzahilmi/saphpi-core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Saphpi\\Controllers\\": "controllers/",
            "Saphpi\\Rules\\": "rules/",
            "Saphpi\\Models\\": "models/",
            "Saphpi\\Middlewares\\": "middlewares/",
            "Saphpi\\Exceptions\\": "exceptions/"
        }
    },
    "minimum-stability": "dev"
}
